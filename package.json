{
  "name": "@reallifedigital/nuxt-image-loader-module",
  "version": "1.4.1",
  "description": "An image loader module for nuxt.js that allows you to configure image style derivatives.",
  "main": "index.js",
  "files": [
    "dist/macros.js",
    "src/plugin.template.js"
  ],
  "scripts": {
    "test:e2e:build": "cd tests/nuxt && npm run build",
    "test:e2e:start": "cd tests/nuxt && npm run start",
    "test:e2e:buildStatic": "cd tests/nuxt && npm run generate",
    "test:e2e:serveStatic": "http-server tests/nuxt/dist -a 127.0.0.1 -p 9001",
    "test:e2e:runServer": "cypress run --spec cypress/integration/validImagesServer.spec.js",
    "test:e2e:runStatic": "cypress run --spec cypress/integration/validImagesStatic.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reallifedigital/nuxt-image-loader-module.git"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "image-processing",
    "images",
    "graphicsmagick",
    "module"
  ],
  "author": "Barry Fisher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reallifedigital/nuxt-image-loader-module/issues"
  },
  "homepage": "https://github.com/reallifedigital/nuxt-image-loader-module#readme",
  "dependencies": {
    "glob": "^7.1.4",
    "gm": "^1.23.1",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "@types/glob": "^7.1.2",
    "@types/gm": "^1.18.4",
    "@types/jquery": "^3.3.31",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.0.11",
    "cypress": "^4.7.0",
    "http-server": "^0.12.3"
  }
}
